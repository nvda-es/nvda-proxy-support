msgstr "Servidor: "

#. TRANSLATORS: proxy port for socks connections
#. TRANSLATORS: proxy port for http connections
#. TRANSLATORS: proxy port for https connections
#. TRANSLATORS: proxy port for ftp connections
msgid "Port: "
msgstr "Porta: "

